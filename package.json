{
  "name": "@yelaiii/stylelint",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.18.1",
  "description": "Stylelint config",
  "author": "iiivanpopov",
  "license": "ISC",
  "keywords": [
    "stylelint",
    "config",
    "sort",
    "css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "pnpm eslint .",
    "build": "tsdown --dts",
    "prepublishOnly": "pnpm build",
    "prepare": "husky"
  },
  "peerDependencies": {
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.0"
  },
  "dependencies": {
    "local-pkg": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/node": "^24.7.0",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*": "pnpm eslint --fix"
  }
}
